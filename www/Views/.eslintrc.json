{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "ignorePatterns": [
        "*.min.js"
    ],
    "rules": {
        "indent": [
            "error",
            "tab"
        ],
        "no-tabs": 0
    }
}